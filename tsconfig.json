{
  "exclude": [],
  "compilerOptions": {
    "target": "ES2021",
    "lib": [
      "ES2021"
    ],
    "module": "CommonJS",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "strictPropertyInitialization": false,
    "outDir": "./build",
    "rootDir": "./",
    "baseUrl": "src",
  }
}